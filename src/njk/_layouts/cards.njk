{% extends '_layouts/base.njk' %}

{% block content %}

<div class="card mb-3" style="max-width: 540px;">
  <div class="row no-gutters">
    <div class="col-md-4">
      <img src="../img/members/freddie-mercury-color.jpg" class="card-img" alt="A photo of Freddie Mercury Singing">
    </div>
    <div class="col-md-8">
      <div class="card-body">
        <h5 class="card-title">{% block name %} Test {% endblock %}</h5>
        <p class="card-text">
          <ul> 


        {% block story %}{% endblock %}
        </p>
        <p class="card-text"><small class="text-muted">Last updated 3 mins ago</small></p>
      </div>
    </div>
  </div>
</div>

{% endblock %}